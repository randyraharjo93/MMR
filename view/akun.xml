									<field name="nomorakun" />
									<field name="namaakun" />
									<field name="debit" />
									<field name="kredit" />
									<field name="normaldi" />
								</tree> 
								<form string="Jurnal" version="7.0" >
									<group>
										<group string="Informasi Dasar">
											<field name="idakunparent"/>
											<field name="nomorakun"/>
											<field name="namaakun"/>
											<field name="normaldi"/>
										</group>
										<group string="Informasi Saldo">
											<field name="debit"/>
											<field name="kredit"/>
										</group>
									</group>
									<notebook>
										<page string="History">
											<field name="akundetil">
												<tree string="History" version="7.0" create="false" edit="false" delete="false" default_order="tanggal">
													<field name="tanggal" />
													<field name="sumber" />
													<field name="debit" />
													<field name="kredit" />
												</tree> 
												<form string="Aturan Akun" version="7.0" >
													<group>
														<group string="Informasi Dasar">
															<field name="tanggal"/>
															<field name="sumber"/>
														</group>
														<group string="Informasi Saldo">
															<field name="debit"/>
															<field name="kredit"/>
														</group>
													</group>
												</form>
											</field>	
										</page>	
									</notebook>
								</form>
							</field>
						</page>
						<page string="Jurnal Penyesuaian">
							<group style="background-color:#e5f2ff">
